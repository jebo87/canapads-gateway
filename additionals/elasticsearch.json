{
    "_source": [
        "id",
        "title",
        "description",
        "published_date",
        "price",
        "last_updated"
    ],
    "size": 100,
    "query": {
        "bool": {
            "must": [
                {
                    "match": {
                        "title": {
                            "query": "Sparkling flat",
                            "boost": 2
                        }
                    }
                }
            ],
            "filter": [
                {
                    "term": {
                        "gym": true
                    }
                },
                {
                    "range": {
                        "price": {
                            "gte": 1500,
                            "lte": 2000
                        }
                    }
                }
            ]
        }
    }
}